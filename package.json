{
  "name": "aurelia-typescript-webpack-starter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --inline --hot --open",
    "build": "webpack --progress --hide-modules",
    "test": "karma start test/karma.conf.js --singleRun",
    "test:watch": "karma start test/karma.conf.js",
    "e2e": "npm run e2e:install && npm run e2e:run",
    "e2e:install": "webdriver-manager update",
    "e2e:run": "protractor test/protractor.conf.js",
    "e2e:debug": "protractor test/protractor.conf.js --elementExplorer",
    "clean": "rm -rf dist/*"
  },
  "dependencies": {
    "aurelia-bootstrapper": "2.1.1",
    "aurelia-event-aggregator": "1.0.1",
    "aurelia-framework": "1.1.2",
    "aurelia-history-browser": "1.0.0",
    "aurelia-logging-console": "1.0.0",
    "aurelia-templating-binding": "1.3.0",
    "aurelia-templating-resources": "1.4.0",
    "aurelia-templating-router": "1.1.0"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.47",
    "@types/webpack": "2.2.14",
    "@types/webpack-env": "1.13.0",
    "aurelia-protractor-plugin": "https://github.com/aurelia/protractor-plugin",
    "aurelia-testing": "1.0.0-beta.3.0.1",
    "aurelia-webpack-plugin": "2.0.0-rc.2",
    "awesome-typescript-loader": "3.1.2",
    "html-loader": "0.4.5",
    "jasmine-core": "2.5.2",
    "karma": "1.6.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-jasmine": "1.1.0",
    "karma-spec-reporter": "0.0.30",
    "karma-typescript": "github:monounity/karma-typescript",
    "protractor": "5.1.1",
    "ts-node": "3.0.2",
    "typescript": "2.2.2",
    "webpack": "2.3.3",
    "webpack-dev-server": "2.4.2"
  },
  "peerDependencies": {}
}
