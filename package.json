{
  "name": "aurelia-typescript-webpack-starter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server -d --inline --hot --open --watch",
    "start": "webpack-dev-server -d --hot --config webpack.config.js --watch",
    "build": "webpack --progress --hide-modules",
    "clean": "rm -rf dist/*"
  },
  "dependencies": {
    "aurelia-bootstrapper": "2.1.1",
